<%= form_with(model: match) do |f| %>  
  <%= render "shared/errors", object: match %>

  <%= f.label :local_team %>
  <%= f.text_field :local_team %>

  <%= f.label :visiting_team %>
  <%= f.text_field :visiting_team %>

  <%= f.label :started_at %>
  <%= f.datetime_field :started_at %>

  <%= f.label :ended_at %>
  <%= f.datetime_field :ended_at %>  

   <%= f.submit %> 
  
<% end %>